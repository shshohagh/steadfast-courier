{
    "name": "shshohagh/steadfast-courier",
    "description": "A Laravel package for Steadfast Courier integration",
    "type": "library",
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Shshohagh\\SteadfastCourier\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shshohagh\\SteadfastCourier\\SteadfastServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
